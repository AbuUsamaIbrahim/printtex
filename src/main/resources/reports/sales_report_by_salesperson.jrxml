    <queryString>
        <![CDATA[SELECT
DATE_FORMAT(s.sale_date,"%d/%m/%y")sale_date,
sp.salesperson_name,cu.customer_name,
c.company_name,s.sale_unit_price,
b.bill_no,
s.total_sale_amount,
s.sale_amount,
b.previous_due_amount,
s.sale_amount+b.previous_due_amount total_payable,
b.paid_amount,
b.new_due_amount new_due
FROM item i,sale s, bill b, salesperson sp,company c, customer cu
WHERE i.item_id=s.item_id AND s.bill_id=b.bill_id AND sp.salesperson_id=s.salesperson_id AND s.customer_id=cu.customer_id AND s.company_id=c.company_id and ($P{item_id} is null or s.salesperson_id=$P{item_id}) and ($P{branch_id} is null or s.branch_id=$P{branch_id})]]>
    </queryString>
